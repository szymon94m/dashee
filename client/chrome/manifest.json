{
  "manifest_version": 2,
  "name": "Dashee",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "./styles/img/icon_16.png",
    "96": "./styles/img/icon_96.png",
    "128": "./styles/img/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    {"socket": [
      "udp-send-to",
      "udp-bind"
    ]}
  ]
}
